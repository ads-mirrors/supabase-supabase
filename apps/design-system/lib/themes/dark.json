  "semanticTokenColors": {
    "comment": {
      "foreground": "#fff4"
    },
    "keyword": {
      "foreground": "#fff8"
    },
    "string": {
      "foreground": "#fff8"
    },
    "selfKeyword": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "method.declaration": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "method.definition": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "method": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": false
    },
    "function.declaration": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "function.definition": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "function": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": false
    },
    "property": {
      "foreground": "hsl(var(--foreground-default/1))"
    },
    "enumMember": {
      "foreground": "#fff8",
      "bold": false
    },
    "enum": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "boolean": {
      "foreground": "#fff8"
    },
    "number": {
      "foreground": "#fff8"
    },
    "type": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "typeAlias": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "class": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "selfTypeKeyword": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "builtinType": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "interface": {
      "foreground": "#fff8",
      "bold": false
    },
    "typeParameter": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    },
    "lifetime": {
      "foreground": "#fff8",
      "italic": false,
      "bold": false
    },
    "namespace": {
      "foreground": "hsl(var(--foreground-default/1))"
    },
    "macro": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": false
    },
    "decorator": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": false
    },
    "builtinAttribute": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": false
    },
    "generic.attribute": {
      "foreground": "hsl(var(--foreground-default/1))"
    },
    "derive": {
      "foreground": "hsl(var(--foreground-default/1))"
    },
    "operator": {
      "foreground": "#fff8"
    },
    "variable": {
      "foreground": "hsl(var(--foreground-default/1))"
    },
    "variable.readonly": {
      "foreground": "#fff8"
    },
    "parameter": {
      "foreground": "hsl(var(--foreground-default/1))"
    },
    "variable.mutable": {
      "underline": true
    },
    "parameter.mutable": {
      "underline": true
    },
    "selfKeyword.mutable": {
      "underline": true
    },
    "variable.constant": {
      "foreground": "#fff8"
    },
    "struct": {
      "foreground": "hsl(var(--foreground-default)/1)",
      "bold": true
    }
  },
  "tokenColors": [
    {
      "name": "Fallback Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#fff8"
      }
    },
    {
      "name": "Fallback keywords",
      "scope": [
        "storage.type.ts",
        "keyword",
        "keyword.other",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#fff8"
      }
    },
    {
      "name": "Fallback strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#fff8"
      }
    },
    {
      "name": "Fallback JSON Properties",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "hsl(var(--foreground-default/1))"
      }
    },
    {
      "name": "Fallback string variables",
      "scope": ["string variable", "string meta.interpolation"],
      "settings": {
        "foreground": "hsl(var(--foreground-default/1))"
      }
    },
    {
      "name": "Fallback comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#fff4"
      }
    },
    {
      "name": "Fallback constants",
      "scope": ["constant"],
      "settings": {
        "foreground": "#fff8"
      }
    },
    {
      "name": "Fallback self/this",
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "hsl(var(--foreground-default/1))"
      }
    },
    {
      "name": "Fallback types",
      "scope": [
        "entity.other.alias",
        "source.php support.class",
        "entity.name.type",
        "meta.function-call support.class",
        "keyword.other.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "hsl(var(--foreground-default)/1)",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Fallback method calls",
      "scope": ["meta.method-call entity.name.function"],
      "settings": {
        "foreground": "hsl(var(--foreground-default)/1)",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback function calls",
      "scope": [
        "meta.function-call entity.name.function",
        "meta.function-call support.function",
        "meta.function.call entity.name.function"
      ],
      "settings": {
        "foreground": "hsl(var(--foreground-default)/1)",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback enums & constants",
      "scope": ["constant.enum", "constant.other"],
      "settings": {
        "foreground": "#fff8"
      }
    },
    {
      "name": "Fallback Properties & func arguments",
      "scope": [
        "variable.other.property",
        "entity.name.goto-label",
        "entity.name.variable.parameter"
      ],
      "settings": {
        "foreground": "hsl(var(--foreground-default/1))"
      }
    },
    {
      "name": "Fallback functions & methods declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.function.constructor",
        "entity.name.function meta.function-call meta.method-call"
      ],
      "settings": {
        "foreground": "hsl(var(--foreground-default)/1)",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["meta.tag entity.name.tag.html", "entity.name.tag.template.html"],
      "settings": {
        "foreground": "hsl(var(--foreground-default/1))"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#fff8"
      }
    },
    {
      "name": "HTML Custom Tag",
      "scope": ["meta.tag.other.unrecognized.html entity.name.tag.html"],
      "settings": {
        "foreground": "hsl(var(--foreground-default/1))"
      }
    },
    {
      "name": "HTML Keywords",
      "scope": ["text.html keyword"],
      "settings": {
        "foreground": "hsl(var(--foreground-default/1))"
      }
    },
    {
      "name": "Punctuations",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#fff8"
      }
    }
  ]
}
