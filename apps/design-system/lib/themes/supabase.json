{
  "name": "Supabase Theme",
  "displayName": "Supabase Theme",
  "semanticHighlighting": true,
  "colors": {
    "editorLink.activeForeground": "var(--code-token-link)",
    "foreground": "var(--code-foreground)",
    "button.background": "var(--code-foreground)",
    "button.foreground": "#000",
    "button.hoverBackground": "var(--code-foreground)",
    "list.highlightForeground": "var(--code-foreground)",
    "textLink.foreground": "var(--code-foreground)",
    "scrollbar.shadow": "#000",
    "textLink.activeForeground": "var(--code-foreground)",
    "editor.lineHighlightBackground": "var(--code-highlight-color)",
    "editor.lineHighlightBorder": "var(--code-highlight-color)",
    "editorCursor.foreground": "var(--code-foreground)",
    "editor.findMatchBackground": "var(--code-foreground)",
    "editor.findMatchHighlightBackground": "var(--code-foreground)",
    "list.activeSelectionForeground": "var(--code-foreground)",
    "list.focusForeground": "var(--code-foreground)",
    "list.hoverForeground": "var(--code-foreground)",
    "list.inactiveSelectionForeground": "var(--code-foreground)",
    "list.inactiveSelectionBackground": "var(--code-highlight-color)",
    "list.focusBackground": "var(--code-highlight-color)",
    "list.focusAndSelectionOutline": "var(--code-highlight-color)",
    "list.focusHighlightForeground": "var(--code-foreground)",
    "list.hoverBackground": "var(--code-highlight-color)",
    "list.focusOutline": "var(--code-highlight-color)",
    "list.activeSelectionBackground": "var(--code-highlight-color)",
    "editorIndentGuide.background": "var(--code-foreground)",
    "editor.background": "var(--code-highlight-color)",
    "editor.foreground": "var(--code-foreground)",
    "editor.foldBackground": "var(--code-highlight-color)",
    "editor.hoverHighlightBackground": "var(--code-highlight-color)",
    "editor.selectionBackground": "var(--code-highlight-color)",
    "editor.inactiveSelectionBackground": "var(--code-highlight-color)",
    "gitDecoration.modifiedResourceForeground": "var(--code-foreground)",
    "gitDecoration.untrackedResourceForeground": "var(--code-token-constant)",
    "gitDecoration.conflictingResourceForeground": "var(--code-token-string-expression)",
    "gitDecoration.deletedResourceForeground": "var(--code-token-comment)",
    "listFilterWidget.background": "var(--code-highlight-color)",
    "input.background": "var(--code-foreground)",
    "titleBar.activeForeground": "var(--code-foreground)",
    "editorWidget.background": "var(--code-highlight-color)",
    "editorGutter.background": "var(--code-highlight-color)",
    "debugToolBar.background": "var(--code-highlight-color)",
    "commandCenter.background": "var(--code-highlight-color)",
    "sideBarSectionHeader.background": "var(--code-highlight-color)",
    "focusBorder": "var(--code-foreground)",
    "titleBar.activeBackground": "var(--code-highlight-color)",
    "titleBar.inactiveBackground": "var(--code-highlight-color)",
    "breadcrumb.background": "var(--code-highlight-color)",
    "activityBar.background": "var(--code-highlight-color)",
    "activityBar.foreground": "var(--code-foreground)",
    "panel.background": "var(--code-highlight-color)",
    "sideBar.background": "var(--code-highlight-color)",
    "sideBarTitle.foreground": "var(--code-foreground)",
    "tab.hoverBackground": "var(--code-highlight-color)",
    "terminal.background": "var(--code-highlight-color)",
    "statusBar.background": "var(--code-highlight-color)",
    "statusBar.foreground": "var(--code-foreground)",
    "selection.background": "var(--code-foreground)",
    "editorPane.background": "var(--code-highlight-color)",
    "badge.background": "var(--code-highlight-color)",
    "banner.background": "var(--code-highlight-color)",
    "menu.background": "var(--code-highlight-color)",
    "activityBarBadge.background": "var(--code-highlight-color)",
    "activityBarBadge.foreground": "var(--code-foreground)",
    "editorLineNumber.foreground": "var(--code-foreground)",
    "editorLineNumber.activeForeground": "var(--code-foreground)",
    "statusBarItem.errorBackground": "var(--code-token-comment)"
  },
  "semanticTokenColors": {
    "comment": {
      "foreground": "var(--code-token-comment)"
    },
    "keyword": {
      "foreground": "var(--code-token-keyword)"
    },
    "string": {
      "foreground": "var(--code-token-string)"
    },
    "selfKeyword": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "method.declaration": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "method.definition": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "method": {
      "foreground": "var(--code-foreground)",
      "bold": false
    },
    "function.declaration": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "function.definition": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "function": {
      "foreground": "var(--code-foreground)",
      "bold": false
    },
    "property": {
      "foreground": "var(--code-token-property)"
    },
    "enumMember": {
      "foreground": "var(--code-token-keyword)",
      "bold": false
    },
    "enum": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "boolean": {
      "foreground": "var(--code-token-keyword)"
    },
    "number": {
      "foreground": "var(--code-token-number)"
    },
    "type": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "typeAlias": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "class": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "selfTypeKeyword": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "builtinType": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "interface": {
      "foreground": "var(--code-token-keyword)",
      "bold": false
    },
    "typeParameter": {
      "foreground": "var(--code-foreground)",
      "bold": true
    },
    "lifetime": {
      "foreground": "var(--code-token-keyword)",
      "italic": false,
      "bold": false
    },
    "namespace": {
      "foreground": "var(--code-foreground)"
    },
    "macro": {
      "foreground": "var(--code-foreground)",
      "bold": false
    },
    "decorator": {
      "foreground": "var(--code-foreground)",
      "bold": false
    },
    "builtinAttribute": {
      "foreground": "var(--code-foreground)",
      "bold": false
    },
    "generic.attribute": {
      "foreground": "var(--code-foreground)"
    },
    "derive": {
      "foreground": "var(--code-foreground)"
    },
    "operator": {
      "foreground": "var(--code-token-keyword)"
    },
    "variable": {
      "foreground": "var(--code-foreground)"
    },
    "variable.readonly": {
      "foreground": "var(--code-token-keyword)"
    },
    "parameter": {
      "foreground": "var(--code-token-parameter)"
    },
    "variable.mutable": {
      "underline": true
    },
    "parameter.mutable": {
      "underline": true
    },
    "selfKeyword.mutable": {
      "underline": true
    },
    "variable.constant": {
      "foreground": "var(--code-token-keyword)"
    },
    "struct": {
      "foreground": "var(--code-foreground)",
      "bold": true
    }
  },
  "tokenColors": [
    {
      "name": "Fallback Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "var(--code-token-keyword)"
      }
    },
    {
      "name": "Fallback keywords",
      "scope": [
        "storage.type.ts",
        "keyword",
        "keyword.other",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "var(--code-token-keyword)"
      }
    },
    {
      "name": "Fallback strings",
      "scope": ["string"],
      "settings": {
        "foreground": "var(--code-token-string)"
      }
    },
    {
      "name": "Fallback JSON Properties",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "var(--code-token-property)"
      }
    },
    {
      "name": "Fallback string variables",
      "scope": ["string variable", "string meta.interpolation"],
      "settings": {
        "foreground": "var(--code-token-string)"
      }
    },
    {
      "name": "Fallback comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "var(--code-token-comment)"
      }
    },
    {
      "name": "Fallback constants",
      "scope": ["constant"],
      "settings": {
        "foreground": "var(--code-token-constant)"
      }
    },
    {
      "name": "Fallback self/this",
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "var(--code-foreground)"
      }
    },
    {
      "name": "Fallback types",
      "scope": [
        "entity.other.alias",
        "source.php support.class",
        "entity.name.type",
        "meta.function-call support.class",
        "keyword.other.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "var(--code-foreground)",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Fallback method calls",
      "scope": ["meta.method-call entity.name.function"],
      "settings": {
        "foreground": "var(--code-foreground)",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback function calls",
      "scope": [
        "meta.function-call entity.name.function",
        "meta.function-call support.function",
        "meta.function.call entity.name.function"
      ],
      "settings": {
        "foreground": "var(--code-foreground)",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback enums & constants",
      "scope": ["constant.enum", "constant.other"],
      "settings": {
        "foreground": "var(--code-token-keyword)"
      }
    },
    {
      "name": "Fallback Properties & func arguments",
      "scope": [
        "variable.other.property",
        "entity.name.goto-label",
        "entity.name.variable.parameter"
      ],
      "settings": {
        "foreground": "var(--code-token-property)"
      }
    },
    {
      "name": "Fallback functions & methods declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.function.constructor",
        "entity.name.function meta.function-call meta.method-call"
      ],
      "settings": {
        "foreground": "var(--code-foreground)",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["meta.tag entity.name.tag.html", "entity.name.tag.template.html"],
      "settings": {
        "foreground": "var(--code-foreground)"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "var(--code-token-keyword)"
      }
    },
    {
      "name": "HTML Custom Tag",
      "scope": ["meta.tag.other.unrecognized.html entity.name.tag.html"],
      "settings": {
        "foreground": "var(--code-foreground)"
      }
    },
    {
      "name": "HTML Keywords",
      "scope": ["text.html keyword"],
      "settings": {
        "foreground": "var(--code-foreground)"
      }
    },
    {
      "name": "Punctuations",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "var(--code-token-punctuation)"
      }
    }
  ]
}
